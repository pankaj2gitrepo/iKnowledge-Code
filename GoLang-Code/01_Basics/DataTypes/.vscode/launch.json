{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "float",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/float",
        },
        {
            "name": "generics",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/generics",
            "dlvFlags": ["--check-go-version=false"] 
        },
        {
            "name": "other",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/other",
        },
        {
            "name": "slice",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/slice",
            "dlvLoadConfig":{
                "followPointers": true,
                "maxVariableRecurse": 1,
                "maxStringLen": 512,
                "maxArrayValues": 64,
                "maxStructFields": -1
              },
             "dlvFlags": ["--check-go-version=false"] 
        },
    ]
}